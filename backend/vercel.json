{
    "version": 2,
    "builds": [
      { "src": "api/send-email.js", "use": "@vercel/node" },
      { "src": "../frontend/dist/**/*", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/api/send-email", "dest": "/api/send-email.js" },
      { "src": "/(.*)", "dest": "../frontend/dist/$1" }
    ]
  }
  